{
  "version": 2,
  "alias": "tdd-in-react",
  "builds": [
    { "src": "./presentation/deck.mdx", "use": "@now/mdx-deck" },
    { "src": "./app/next.config.js", "use": "@now/next" }
  ],
  "public": true,
  "github": {
    "enabled": true,
    "autoAlias": true
  },
  "routes": [
    { "src": "/main.js", "dest": "/presentation/main.js" },
    { "src": "/images/(.*)", "dest": "/presentation/images/$1" },
    { "src": "/presentation/(.*)", "dest": "/presentation/index.html" },
    { "src": "/(.*)", "dest": "/app/$1" }
  ]
}
